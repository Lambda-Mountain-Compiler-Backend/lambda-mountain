
import STDLIB/default-instruction-set.lm;
import STDLIB/default-primitives.lm;
import STDLIB/default-stdlib.lm;
import STDLIB/default-rules.lm;

global-x := (: SNil S);

main := (tail(
   (let x (close SNil))
   (let y (close SNil))
   (set global-x (SCons(
      x
      y
   )))
   (print global-x)
));
