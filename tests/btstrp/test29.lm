type X (X( U64 , U64 ));

f := λ. (: (tail(
   (let x (X( 7_u64 8_u64 )))
   (let y (X( 12_u64 13_u64 )))
   y
)) X);

f1 := λ. 3_u8;
f2 := λ. 4_u16;
f3 := λ. 5_u32;
f4 := λ. 6_u64;

main := λ. (: (tail(
   (print (f1()))(print '\n_s)
   (print (f2()))(print '\n_s)
   (print (f3()))(print '\n_s)
   (print (f4()))(print '\n_s)
   (let x (f()))
   (print (.0 x))(print '\n_s)
   (print (.1 x))(print '\n_s)
   (print (.2 x))(print '\n_s)
)) Nil);
