
show-ascript := Î»p. (tail(
   (print-s (ShowAscript p))
   (local term)
   (set term (match p (
      ()
      ( () () )
      ( (App( f x )) (
         (App( (show-ascript f) (show-ascript x) ))
      ))
      ( (prog (Global( n v ))) (
         ( (show-ascript prog) (Global( n (show-ascript v) )) )
      ))
      ( unknown (
         (UnknownTerm unknown)
      ))
   )))
   term
));

main := print-s (show-ascript (() (Global ('main (App ((Variable 'print-s) (Variable 'argv)))))));
