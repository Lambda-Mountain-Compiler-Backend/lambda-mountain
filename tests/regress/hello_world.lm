
import LIB/default-minimal.lm;

fragment  cdecl::return := λ: DontChain(: l Nil). (: (
   (.program(
      (.program l)
      (.del l)
   ))
   (.frame( (.frame l) ))
) Nil);

main := λ. (: (
   (mov( 1_u64 RAX ))          # 1 is sys_write
   (mov( 1_u64 RDI ))          # 1 is STDOUT
   (mov( 'hello_world_s RSI )) # character buffer
   (mov( 11_u64 RDX ))         # length of data to write
   (syscall())
) Nil);
