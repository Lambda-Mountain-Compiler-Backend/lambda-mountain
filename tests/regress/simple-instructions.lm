(test-declare-simple-instruction-alias-binop-64( do-sub 'subq U64 ));

g1 := 1_u64;
g2 := 2_u64;

f := λ(: l U64)(: r U64). (: (
   (+( l r ))
) U64);

main := λ. (: (
   (let l1 1_u64)
   (let l2 2_u64)
   (print(do-sub( l2 l1 ))) # 1
   (print(do-sub( g2 l1 ))) # 1
   (print(do-sub( l2 g1 ))) # 1
   (print(do-sub( (f( 3_u64 3_u64 )) 5_u64 ))) # 1
   (print(do-sub( 7_u64 (f( 3_u64 3_u64 )) ))) # 1
) Nil);
