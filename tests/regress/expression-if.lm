
import LIB/default.lm;

e1 := λ(: x U64). (: (
  (if (==( x 0_u64 )) 1_u64 2_u64)
) U64);

e2 := λ(: x U64). (: (
  (match x (
     ()
     ( 0_u64 3_u64 )
     ( _ 4_u64 )
  ))
) U64);

main := λ. (: (tail(
  (print (e1 0_u64))
  (print (e1 5_u64))
  (print '\n_s)

  (print (e2 0_u64))
  (print (e2 5_u64))
  (print '\n_s)
)) Nil);

