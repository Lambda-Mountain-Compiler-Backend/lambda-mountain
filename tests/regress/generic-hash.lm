
import LIB/default.lm;

generic-hash := λ(: x Sized<data-size>). (: (tail(
  (let di 0_u64)
  (let x 2_u64)
  (while (<( di (/( data-size_u64 8_u64 )) )) (tail(
     (set x (+( x di )))
     (set di (+( di 1_u64 )))
  )))
  x
)) U64);

main := λ. (: (tail(
  (print(generic-hash( SNil )))
  (print(generic-hash( (SAtom 'abc_s) )))
)) Nil);

