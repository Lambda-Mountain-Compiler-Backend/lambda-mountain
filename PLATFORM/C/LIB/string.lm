
fragment type String; size String 8; atom suffix String _s;

head-string := λ(: x String). (: (
   ([]( (as x U8[]) 0_u64 ))
) U8);

tail-string := λ(: x String). (: (
   (+( (as x U8[]) 1_u64 ))
) String);

== := λ(: l String)(: r String). (: (
   (!=( (strcmp( (as l U8[]) (as r U8[]) )) 0_u32 ))
) U64);
