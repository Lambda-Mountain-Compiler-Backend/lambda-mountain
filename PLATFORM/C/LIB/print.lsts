
let print(e: x+DefaultPrintable<x>): Nil = (
   print( stdout(), e );
);

let eprint(e: x+DefaultPrintable<y>): Nil = (
   print( stderr(), e );
);

let fprint(io: IO::File, e: x+DefaultPrintable<y>): Nil = (
   print( io, e );
);

interface self implements DefaultPrintable {
   let print( f: IO::File, p: self ): Nil;
}
