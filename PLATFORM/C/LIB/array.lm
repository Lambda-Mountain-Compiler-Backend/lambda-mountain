
[] := λ: Blob(: x Array<base-type,?>)(: i U64). (: (
   (:frame (:frame x))
   (:program(
      '\[_l x '[_l i ']\]_l
   ))
) base-type);

set[] := λ: Blob(: x Array<base-type,?>)(: i U64)(: v base-type). (: (
   (:frame (:frame x))
   (:program(
      '\[_l x '[_l i ']=_l v '\]_l
   ))
) base-type);
