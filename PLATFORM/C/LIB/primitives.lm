
primitive::while := 位: Blob(: body Meta<Any>)(: cond BranchConditional). (: (
   (:frame( (:frame body) (:frame cond) ))
   (:expression(
      '\[while\[_l (:expression cond) '\]{_l (:expression body) '}\]_l
   ))
) Nil);

primitive::if := 位: Blob(: ictx ImplicitContext)(: f Meta<Any>)(: t Meta<Any>)(: c BranchConditional). (: (
   (:frame( (:frame c) (:frame t) (:frame f) ))
   (:expression(
      '\[_l (:expression c) '?_l (:expression t) ':_l (:expression f) '\]_l
   ))
) Nil);

mov := 位: Blob(: x datatype)(: y datatype+LocalVariable). (: (
   (:frame( (:frame x) (:frame y) ))
   (:expression(
      
   ))
