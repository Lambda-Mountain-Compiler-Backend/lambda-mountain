

::yield-program := 位program. (
   .global _start \n
   .text \n
   _start:
   \t mov $0 , %rdi   \n # set exit status to 0
   \t mov $60 , %rax) \n # system call 60 is exit
   \t syscall         \n # invoke operating system
);

::yield-expression := 位block. (
   TODO implement yield-expression
);

::safe-compile-expression := 位expr rett. (
   ::yield-expression ()
);

::safe-compile-program := 位symbols rett. (
   ::yield-program ()
);
