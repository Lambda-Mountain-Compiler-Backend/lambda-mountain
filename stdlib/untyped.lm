

::yield-program := 位program. (
   (.global _start)
   (.text)
   (label _start)
   (mov $0 %rdi)  # set exit status to 0
   (mov $60 %rax) # system call 60 is exit
   (syscall)      # invoke operating system
);

::yield-expression := 位block. (
   TODO implement yield-expression
);

::safe-compile-expression := 位expr rett. (
   ::yield-expression ()
);

::safe-compile-program := 位symbols rett. (
   ::yield-program ()
);
