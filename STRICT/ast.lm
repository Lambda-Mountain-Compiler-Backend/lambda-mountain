
type AST ASTNil | (App( AST[] , AST[] )) | (Var( String )) | (Lit( String )) | (Abs( AST[] , AST[] )) | (Asc( AST[] , Type )) | (Seq( AST[] , AST[] ));

ast-tokenized-program := (: SNil S);
ast-parsed-program := (: ASTNil AST);

close := λ(: x AST). (: (tail(
   (mov( (malloc(sizeof AST)) R8 ))
   (mov( x 0_u64 (as R8 AST[]) ))
   (as R8 AST[])
)) AST[]);

print := λ(: t AST). (: (tail(
   (match t (
      ()
      ( ASTNil (print '\[\]_s) )
      ( (Var a) (print a) )
      ( (Lit a) (tail( (print '\`_s) (print a) )))
      ( (App( l r )) (tail(
         (print '\[_s)
         (print l)
         (print '\s_s)
         (print r)
         (print '\]_s)
      )))
      ( (Abs( lhs rhs )) (tail(
         (print '\[_s)
         (print '\l_s) (print lhs)
         (print lhs)
         (print '._s) (print rhs)
         (print rhs)
         (print '\]_s)
      )))
#      ( (Asc( at att )) (tail(
#         (print '\[_s)
#         (print '\s:\s_s)
#         (print at)
#         (print '\s_s)
#         (print att)
#         (print '\]_s)
#      )))
      ( (Seq( l r )) (tail(
         (print l)
         (print '\:\n_s)
         (print r)
      )))
   ))
   ()
)) Nil);
