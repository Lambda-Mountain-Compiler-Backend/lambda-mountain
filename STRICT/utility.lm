
list-tail-order-to-head-order := λ(: s S). (: (tail(
   (let new (close SNil))
   (while (non-zero s) (match s (
      ()
      ( (SCons( l r )) (tail(
         (set new (close(SCons(
            (close r)
            new
         ))))
         (set s l)
      )))
   )))
   new
)) S[]);

is-variable := λ(: s String). (: (tail(
   ()
   0_u64
)) U64);
