
let .slot(tt: Type, sl: CString): Type = (
   match tt {
      TGround{ tag=tag } => (
         if tag==sl then tt else TAny{}
      );
      TAnd{ conjugate=conjugate } => (
         let return = TAny;
         for c in conjugate { if not(non-zero(return)) then return = c.slot(sl) };
         return
      );
      _ => TAny{};
   }
);
