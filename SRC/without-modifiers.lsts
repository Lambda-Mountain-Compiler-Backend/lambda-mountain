
let .without-modifiers(tt: Type): Type = (
   match tt {
      TAnd { conjugate=conjugate } => (
         let result = TAny;
         for vector c in conjugate { result = result && c.without-modifiers };
         result
      );
      TGround { tag:c"C", parameters:[TGround{tag:c"extern"}..] } => TAny;
      _ => tt;
   }
);
