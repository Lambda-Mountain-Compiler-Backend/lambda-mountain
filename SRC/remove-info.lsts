
let remove-info(base: Type, rm: Type): Type = (
   match base {
      TAnd{ conjugate=conjugate } => (
         let result = TAny;
         for vector c in conjugate { result = result && remove-info(c,rm) };
         result;
      );
      TGround{} => (
         if can-unify(rm,base) then TAny else base
      );
      _ => base;
   }
);
