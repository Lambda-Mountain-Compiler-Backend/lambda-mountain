
print := λ(: t Token). (: (
   (match t (
      ()
      ( (Token( v _ _ )) (print v) )
   ))
) Nil);

print := λ(: loc SourceLocation). (: (
   (match loc (
      ()
      ( (SourceLocation( fp ln cl )) (
         (print 'In\sFile\s_s)
         (print fp)
         (print '\sLine\s_s)
         (print ln)
         (print '\sColumn\s_s)
         (print cl)
      ))
   ))
) Nil);

