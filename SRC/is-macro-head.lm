
is-macro-head := Î»(: s String)(: arity U64). (: (
   (let macros preprocess-macros)
   (let found 0_u64)
   (while (non-zero macros) (match macros (
      ()
      ( (MSeq( rst (Macro( (App( (Lit( mv _ )) p1 )) mrhs )) )) (
         (if (&&( (==( arity 1_u64 )) (==( s mv )) )) (
