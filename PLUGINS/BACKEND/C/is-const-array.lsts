
let .is-const-array(tt: Type): U64 = (
   match tt {
      TAnd{conjugate=conjugate} => (
         let r = false;
         for vector c in conjugate {
            if c.is-const-array then r = true;
         };
         r
      );
      TGround{tag:c"Array", parameters:[TGround{tag:c"Const"}..]} => true;
      _ => false;
   }
);
